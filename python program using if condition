def max(a,b,c):
	if a==b==c:
		print("All three numbers are equal",a)
	elif a==b:
		print("a and b are equal")
	elif b==c:
		print("b anf c are equal")
	elif c ==a:
		print("c and a are equal")
	elif a>b and a>c :
		print(" a is greater")	
	elif b>a and b>c :
		print("b is greater")
	elif c>a and c>b:
		print("c is greater")
a = int(input("Enter the first number"))
b = int(input("Enter the second number"))
c = int(input("Enter the third number"))
max(a,b,c)
